<%- partial('header') %>
	
<body class="web">
	<!--[if lt IE 7]><p class=chromeframe>Your browser is <em>ancient!</em> <a href="http://browsehappy.com/">Upgrade to a different browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to experience this site.</p><![endif]-->
	<div class="logo web-logo">STICKY</div>

	<div class="web-header">
		<h1>Organize your life</h1>
		<h2 class="tk-proxima-nova-sc-osf-ext-cond">Capturing thoughts that stick in realtime, right inside GChat.</h2>
		<a href="/auth/google" class="signon btn btn-large btn-primary"><img src="/img/google_icon.png" /> Sign in with Google</a>
	</div>

	<div class="web-scope">
		<img src="http://d3sy8nn9suvu9a.cloudfront.net/sticky.io/scope-origami.jpg" />
	</div>

	<div class="web-whatis">
		<ul>
			<li>
				<img src="/img/capture_icon.png" class="screenshot" />
				<h1>Capture your thoughts</h1>
				<div class="content">
					Sticky allows you to capture your thoughts when they happen, how they happen, why 
					they happen.
				</div>
				<div class="equation">+</div>
			</li>
			<li>
				<img src="/img/organize_icon.png" />
				<h1>Organize your ideas</h1>
				<div class="content">
					Organize ideas on your own time, creates an ideal workspace
					for seeing ideas through to completion.
				</div>
				<div class="equation">=</div>
			</li>
			<li>
				<img src="/img/execute_icon.png" />
				<h1>Execute ideas that matter</h1>
				<div class="content">
					With the use of Sticky's UI, developing clarity between ideas has never been easier.	
				</div>
			</li>
		</ul>
	</div>

	<div class="web-ui-dnd">
		<div class="web-content container-fluid">
			<h1>
				Turn ideas into concepts
			</h1>
			<img src="/img/web-ui-drag.png" />
		</div>
	</div>

	<div class="web-ui">
		<div class="web-content container-fluid">
			<div class="item">
				<h1>Special tags</h1>
				<img src="/img/web-ui-todo-circle.jpg" class="screenshot" />
			</div>
			<div class="item">
				<h1>Notebooks</h1>
				<img src="/img/web-ui-notebooks-circle.png" class="screenshot" />
			</div>
			<div class="item">
				<h1>General purpose tagging</h1>
				<img src="/img/web-ui-tags-circle.png" class="screenshot" />
			</div>

			<div id="myCarousel" class="carousel slide" style="display:none">
				<!-- Carousel items -->
				<div class="carousel-inner">
					<div class="active item">
						<h1>Special tags</h1>
						<img src="/img/web-ui-todo.jpg" class="screenshot" />
					</div>
					<div class="item">
						<h1>Notebooks</h1>
						<img src="/img/web-ui-notebooks-circle.png" class="screenshot" />
					</div>
					<div class="item">
						<h1>General purpose tagging</h1>
						<img src="/img/web-ui-tags-circle.png" class="screenshot" />
					</div>
				</div>
				<!-- Carousel nav -->
				<a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
				<a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
			</div>

		</div>
	</div>

	<div class="web-closing-statement">
		<div class="web-content">
			<h1>
			 	Start organizing your thoughts and ideas with GChat. 
			</h1>
			<h3 class="">
				(You're on there all day anyway.)
			</h2>
			<a href="/auth/google" class="signon btn btn-large btn-primary"><img src="/img/google_icon.png" /> Sign in with Google</a>
		</div>
	</div>

	<div class="web-footer">
		<div class="web-content container-fluid">
			<div class="logo web-footer-logo">STICKY</div>
			<a href="https://twitter.com/Stickyio" target="_blank" class="social-twitter"></a>
			<a href="http://www.facebook.com/Stickyio" target="_blank" class="social-facebook"></a>
		</div>	
	</div>

	
	<script src="//ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
	<script>window.jQuery || document.write('<script src="javascripts/vendor/jquery-1.7.1.min.js"><\/script>')</script>
	
	<%- js('public') %>

	<script>
		$(document).ready(function() {
			$('.carousel').carousel('pause')
			mixpanel.track('Web Visit');

			$('body').scroll(function() {
				console.log('test');
				if ($(this)[0].scrollHeight - $(this).scrollTop() == $(this).outerHeight()) {
					console.log('end')
					mixpanel.track('Scrolled to end of page');
				}
			});
			
				
				
		})
	</script>


	</body>
</html>
